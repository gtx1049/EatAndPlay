<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:bootstrap="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context="com.gtx.eatandplay.PageContent">

    <com.beardedhen.androidbootstrap.BootstrapButton
        android:id="@+id/content_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="返回"
        bootstrap:bb_icon_right="fa-cutlery"
        bootstrap:bb_type="default"
        />
    <com.beardedhen.androidbootstrap.BootstrapButton
        android:id="@+id/get_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_alignParentRight="true"
        android:text="录入"
        bootstrap:bb_icon_right="fa-refresh"
        bootstrap:bb_type="default"
        />

    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/get_content"></WebView>

</RelativeLayout>
